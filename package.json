{
  "name": "Goobox",
  "version": "0.3.1",
  "description": "Goobox community edition sync app for sia and storj ðŸŽ‰ðŸš€",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "start:debug": "electron . --dev-tools",
    "installer": "electron . --installer",
    "installer:debug": "electron . --installer --dev-tools",
    "build": "webpack",
    "build:watch": "webpack -w",
    "build:production": "NODE_ENV=production webpack -p --mode production --display-used-exports",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm test -- --coverage",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "check": "eslint --ext .js,jsx src __tests__ __mocks__ && stylelint \"src/**/*.css\"",
    "check:fix": "eslint --ext .js,jsx --fix src __tests__ __mocks__ && stylelint --fix \"src/**/*.css\"",
    "pack": "electron-builder -wm --dir",
    "dist": "run-s dist:pre dist:build dist:post",
    "dist:build": "run-s dist:build:*",
    "dist:build:mac": "electron-builder -m --x64",
    "dist:build:win": "electron-builder -w --x64",
    "dist:build:linux": "electron-builder -l --x64",
    "dist:pre": "SYNC_STORJ_VERSION=0.0.28 SYNC_SIA_VERSION=0.2.5 run-p dist:pre:*:*",
    "dist:pre:storj:mac": "mkdir -p tmp/mac/goobox-sync-storj && curl -sSL https://github.com/GooBox/goobox-sync-storj/releases/download/${SYNC_STORJ_VERSION}/goobox-sync-storj-${SYNC_STORJ_VERSION}-darwin.zip -o storj-mac.zip && unzip -u storj-mac.zip -d tmp/mac/goobox-sync-storj && rm storj-mac.zip",
    "dist:pre:storj:win": "mkdir -p tmp/win/goobox-sync-storj && curl -sSL https://github.com/GooBox/goobox-sync-storj/releases/download/${SYNC_STORJ_VERSION}/goobox-sync-storj-${SYNC_STORJ_VERSION}-win64.zip -o storj-win.zip && unzip -u storj-win.zip -d tmp/win/goobox-sync-storj && rm storj-win.zip",
    "dist:pre:storj:linux": "mkdir -p tmp/linux/goobox-sync-storj && curl -sSL https://github.com/GooBox/goobox-sync-storj/releases/download/${SYNC_STORJ_VERSION}/goobox-sync-storj-${SYNC_STORJ_VERSION}-linux64.zip -o storj-linux.zip && unzip -u storj-linux.zip -d tmp/linux/goobox-sync-storj && rm storj-linux.zip",
    "dist:pre:sia:mac": "mkdir -p tmp/mac/goobox-sync-sia && curl -sSL https://github.com/GooBox/goobox-sync-sia/releases/download/v${SYNC_SIA_VERSION}/goobox-sync-sia-${SYNC_SIA_VERSION}-mac.tar.gz | tar -zxv -C tmp/mac/goobox-sync-sia --strip-components 1",
    "dist:pre:sia:win": "mkdir -p tmp/win/goobox-sync-sia && curl -sSL https://github.com/GooBox/goobox-sync-sia/releases/download/v${SYNC_SIA_VERSION}/goobox-sync-sia-${SYNC_SIA_VERSION}-win64.tar.gz | tar -zxv -C tmp/win/goobox-sync-sia --strip-components 1",
    "dist:pre:sia:linux": "mkdir -p tmp/linux/goobox-sync-sia && curl -sSL https://github.com/GooBox/goobox-sync-sia/releases/download/v${SYNC_SIA_VERSION}/goobox-sync-sia-${SYNC_SIA_VERSION}-linux.tar.gz | tar -zxv -C tmp/linux/goobox-sync-sia --strip-components 1",
    "dist:post": "del tmp",
    "update": "run-s update:check update:install update:commit",
    "update:check": "updates -u",
    "update:install": "yarn install",
    "update:commit": "git commit -m \"chore: update dependencies\" -- package.json yarn.lock"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/GooBox/goobox-community-gui.git"
  },
  "author": "Goobox",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/GooBox/goobox-community-gui/issues"
  },
  "homepage": "https://github.com/GooBox/goobox-community-gui",
  "dependencies": {
    "about-window": "^1.12.1",
    "core-js": "^2.6.5",
    "electron-json-storage": "^4.1.6",
    "node-jre": "github:jkawamoto/node-jre",
    "node-notifier": "^5.4.0",
    "react-copy-to-clipboard": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "commander": "^2.19.0",
    "connected-react-router": "^6.3.2",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "del": "^4.0.0",
    "del-cli": "^1.1.0",
    "electron": "^4.1.0",
    "electron-builder": "^20.39.0",
    "electron-log": "^3.0.1",
    "electron-updater": "^4.0.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "font-awesome": "^4.7.0",
    "history": "^4.9.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-fetch-mock": "^2.1.1",
    "js-yaml": "^3.12.2",
    "lint-staged": "^8.1.5",
    "menubar": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "opn": "^5.4.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-awesome-popover": "^3.1.1",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router": "^4.4.0",
    "react-router-dom": "^4.4.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-saga": "^1.0.2",
    "rimraf": "^2.6.3",
    "semver": "^5.6.0",
    "stream-to-string": "^1.2.0",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "svg-url-loader": "^2.3.2",
    "updates": "^7.2.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "optionalDependencies": {
    "fileicon": "^0.2.2"
  }
}
